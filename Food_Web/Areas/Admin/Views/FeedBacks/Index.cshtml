@model IEnumerable<Food_Web.Models.FeedBack>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fistname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lastname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fb_email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fb_phone_number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.subject_name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fistname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fb_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fb_phone_number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.subject_name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Fb_id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Fb_id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Fb_id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
